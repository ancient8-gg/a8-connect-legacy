{
  "name": "@ancient8/connect",
  "version": "1.0.199",
  "author": "Ancient8 Developer <dev@ancient8.gg>",
  "repository": "https://git.ancient8.gg/ancient8-dev/a8-uid/a8-connect",
  "license": "GPL-3.0",
  "sideEffects": false,
  "main": "browser.js",
  "types": "browser.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@craco/craco": "^6.4.5",
    "@csstools/postcss-sass": "^5.0.1",
    "@ethersproject/bytes": "^5.6.1",
    "@ethersproject/logger": "^5.6.0",
    "@ethersproject/strings": "^5.6.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^18.0.0",
    "@types/react-modal": "^3.13.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "^10.4.2",
    "bs58": "^5.0.0",
    "class-validator": "^0.13.2",
    "classnames": "^2.3.1",
    "craco-plugin-scoped-css": "^1.1.1",
    "dotenv": "^16.0.1",
    "eslint": "8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "ethers": "^5.6.8",
    "husky": "^8.0.1",
    "postcss": "^8.4.8",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-app": "^1.1.2",
    "react-dom": "^18.1.0",
    "react-modal": "^3.15.1",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.0.23",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.17",
    "typescript": "4.6.2",
    "webpack": "^5.73.0"
  },
  "scripts": {
    "start:craco": "craco start",
    "build:craco:browser": "NODE_ENV=production MODE=browser craco build",
    "build:craco:server": "NODE_ENV=production MODE=server craco build",
    "test:craco": "NODE_ENV=test craco test",
    "build": "bash scripts/build.sh",
    "start": "bash scripts/start.sh",
    "test": "yarn test:craco",
    "eject": "react-scripts eject",
    "format": "prettier --write ./**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint \"**/*.ts\" \"**/*.tsx\" --fix",
    "prepare": "husky install",
    "gen-dts": "tsc -p tsconfig.library.json",
    "gen-doc:server": "typedoc --name \"A8 Connect\" --readme README.server.md --out ./public/a8-connect/server src/server.ts",
    "gen-doc:browser": "typedoc --name \"A8 Connect\" --readme README.md --out ./public/a8-connect/browser src/browser.ts",
    "gen-doc": "yarn gen-doc:browser && yarn gen-doc:server",
    "gen-css": "tailwindcss build -i ./src/base.tailwind.scss -o ./src/generated.scss",
    "transform-mixin": "NODE_PATH=. node scripts/transform-mixin.js",
    "cleanup-css": "rm -rf ./src/generated.scss",
    "cleanup-dts": "rm -rf dts/",
    "cleanup-build": "rm -rf build/",
    "cleanup-pkg": "NODE_PATH=. node scripts/clean-pkg.js",
    "cleanup": "yarn cleanup-dts && yarn cleanup-pkg && yarn cleanup-build && yarn cleanup-css",
    "publish-lib": "cd dist/ && yarn publish",
    "build-css": "yarn cleanup-css && yarn gen-css && yarn transform-mixin",
    "build-lib": "bash scripts/build.sh",
    "build-lib:dev": "yarn build-lib && rm -rf ../dist/ && cp -r dist/ ../dist"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
